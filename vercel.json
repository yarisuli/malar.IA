{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js",
            "origin":"http://192.168.0.109",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers":{
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Headers": ["Content-Type", "Authorization"]
            }
        }
    ]
}